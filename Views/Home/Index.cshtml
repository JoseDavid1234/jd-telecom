@{
  ViewData["Title"] = "Home Page";
}



<div class="container-index text-center">

  <div class="swiper-container">
    <div class="swiper-wrapper">
      <div class="swiper-slide">
        <img src="/img/carousel-1.jpg" alt="">
      </div>
      <div class="swiper-slide">
        <img src="/img/carousel-2.jpg" alt="">
      </div>
      <div class="swiper-slide">
        <img src="/img/carousel-3.jpg" alt="">
      </div>
      <!-- Agrega más slides según sea necesario -->
    </div>
    <!-- Add Pagination -->
    <div class="swiper-pagination"></div>
    <!-- Add Navigation -->
    <div class="swiper-button-next"></div>
    <div class="swiper-button-prev"></div>
  </div>

  <div class="banda"></div>


  <div class="planes" id="planes">
    <h1>Nuestros Planes</h1>
    <div class="banda-planes">
      <img src="/img/plan-1.png" alt="">
      <img src="/img/plan-2.png" alt="">
      <img src="/img/plan-3.png" alt="">
    </div>
  </div>

  <div class="beneficios">
    <h1>Escoge un proveedor confiable</h1>
    <div class="cartas">
      <div class="carta">
        <img src="~/img/beneficios-1.jpg" alt="">
        <p>99% Tolerancia a fallos</p>
      </div>
      <div class="carta">
        <img src="~/img/beneficios-2.jpg" alt="">
        <p>La mejor infraestructura</p>
      </div>
      <div class="carta">
        <img src="~/img/beneficios-3.jpg" alt="">
        <p>Soporte 24/7</p>
      </div>
    </div>

  </div>

  <div class="cobertura" id="cobertura">
    <iframe src="https://www.google.com/maps/d/embed?mid=1bt4MSj3JECmkRa-byJSYNU3lF9tELWo&ehbc=2E312F" width="1000"
      height="500"></iframe>
  </div>

  <div id="contactanos">
    <script type="text/javascript" src="https://form.jotform.com/jsform/231266663115655"></script>
  </div>
</div>